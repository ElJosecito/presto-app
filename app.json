{
  "expo": {
    "name": "presto-app",
    "slug": "presto-app",
    "scheme": "presto-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Presto-Logo-Blanco-y-Negro.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#6BB239"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/Presto-Logo-White-Version.png",
        "backgroundColor": "#6BB239"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
